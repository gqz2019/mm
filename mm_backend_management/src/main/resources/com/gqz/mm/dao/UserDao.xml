<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gqz.mm.dao.UserDao">

    <select id="findByUserName" resultType="com.gqz.mm.pojo.User" parameterType="string">
        select *from mm.t_user where username=#{username};
    </select>
</mapper>